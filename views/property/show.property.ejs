<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Show Property</title>
</head>
<body>
    <h1>Show Property</h1>
    <p>Title: <%= property.title %></p>
    <p>Description: <%= property.description %></p>
    <p>Location: <%= property.location %></p>
    <p>Price: <%= property.price %></p>
    <p>Bedrooms: <%= property.bedrooms %></p>
    <p>Bathrooms: <%= property.bathrooms %></p>
    <p>Images:</p>
    <ul>
        <% property.images.forEach(function(image) { %>
            <li><img src="<%= image %>"></li>
        <% }); %>
    </ul>
    <p>Rating: <%= property.rating %></p>
    <p>Reviews:</p>
    <ul>
        <% property.reviews.forEach(function(review) { %>
            <li><%= review.text %></li>
        <% }); %>
    </ul>
    <form method="POST" action="/property/<%= property._id %>/delete">
        <button type="submit">Delete Property</button>
    </form>
</body>
</html>