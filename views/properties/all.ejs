<h1>All properties</h1>
<% if (!hopper.length) { %> There is any property <% } %> <% hopper.forEach(property => { %>
<div style="border: 1px solid black; margin: 1rem; padding: 1rem">
  <h2><%=property.title%></h2>
  <h2><%=property.price%></h2>
  <h2><%=property.description%></h2>
  <h2><%=property.location%></h2>
  <a href="/properties/<%- property._id %>">Details</a>
</div>
<% }) %>
<a href="/properties/new">Offer your property for rent</a>
